FUNCTION_BLOCK "v3_FB"
{ S7_Optimized_Access := 'FALSE' }
VERSION : 0.1
   VAR 
       Emptying : Bool := false;
       Activar : Bool := false;
       X : Int;
       Y : Int;
       n : Int;
       tempMatrix_0 : Array[0..0, 0..4] of Bool;
       Filas_0 : Int := 0;
       Columnas_0 : Int := 4;
       matrices : Int := 1;
       ascender : Bool;
       contador : Int;
       Activar_LastValue : Bool := false;
   END_VAR

BEGIN

IF (#Activar = TRUE AND #Activar_LastValue = FALSE) THEN
   #Emptying := TRUE;
   #X := 0;
   #Y := 0;
   #n := 0;
   #ascender := TRUE;
   #contador := 0;
END_IF;

IF #Emptying = TRUE THEN
IF #n = 0 THEN
   IF #tempMatrix_0[#X, #Y] = TRUE THEN
       "v1_TON_1".TON(IN := TRUE,
                       PT := t#3s);

       IF "v1_TON_1".Q = TRUE THEN
           IF #ascender = TRUE THEN
               IF #Y = #Columnas_0 THEN
                   #X := #X + 1;
                   #ascender := FALSE;
               ELSE
                   #Y := #Y + 1;
               END_IF;
           ELSE
               IF #Y = 0 THEN
                   #X := #X + 1;
                   #ascender := TRUE;
               ELSE
                   #Y := #Y - 1;
               END_IF;
           END_IF;
           "v1_TON_1".TON(IN := FALSE,
                           PT := t#3s);
           #contador := #contador + 1;
       END_IF;
   ELSE
       IF #ascender = TRUE THEN
           IF #Y = #Columnas_0 THEN
               #X := #X + 1;
               #ascender := FALSE;
           ELSE
               #Y := #Y + 1;
           END_IF;
       ELSE
           IF #Y = 0 THEN
               #X := #X + 1;
               #ascender := TRUE;
           ELSE
               #Y := #Y - 1;
           END_IF;
       END_IF;
       "v1_TON_1".TON(IN := FALSE,
                       PT := t#3s);
       #contador := #contador + 1;
   END_IF;

   IF (#contador = (#Filas_0 + 1) * (#Columnas_0 + 1)) THEN
       #X := 0;
       #Y := 0;
       #n := #n + 1;
       #contador := 0;
       #ascender := true;
   END_IF;
END_IF;
END_IF;

IF (#n = #matrices) THEN
   #Emptying := FALSE;
   #Activar := FALSE;
END_IF;

#Activar_LastValue := #Activar;

END_FUNCTION_BLOCK

