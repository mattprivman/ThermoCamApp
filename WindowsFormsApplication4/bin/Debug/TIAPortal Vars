           IF #Y = #Columnas_0 THEN
               #X := #X + 1;
               #ascender := FALSE;
           ELSE
               #Y := #Y + 1;
           END_IF;
       ELSE
           IF #Y = 0 THEN
               #X := #X + 1;
               #ascender := TRUE;
           ELSE
               #Y := #Y - 1;
           END_IF;
       END_IF;
       "v1_TON_1".TON(IN := FALSE,
                       PT := t#3s);
       #contador := #contador + 1;
