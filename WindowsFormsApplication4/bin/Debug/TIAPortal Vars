FUNCTION_BLOCK "c2_FB"
{ S7_Optimized_Access := 'FALSE' }
VERSION : 0.1
   VAR_INPUT 
       maxTemp : Int;
   END_VAR

   VAR 
       Cooling : Bool := false;
       Activar : Bool := false;
       Mode    : Bool := false;
       Valvula : Bool := false;
       X : Int;
       Y : Int;
       n : Int;
       Filas_0 : Int := 1;
       Columnas_0 : Int := 1;
       tempMatrix_0 : Array[0..1, 0..1] of Int;
       Filas_1 : Int := 1;
       Columnas_1 : Int := 1;
       tempMatrix_1 : Array[0..1, 0..1] of Int;
       ascender : Bool;
       matrices : Int := 2;
       contador : Int;
   END_VAR

BEGIN

IF #Mode = FALSE THEN //AUTOMATICO

"c2_TON_1".TON(IN := #Activar AND NOT #Cooling,
                 PT := t#10s);

IF "c2_TON_1".Q = TRUE THEN
   #Cooling := TRUE;
   #Activar := FALSE;
   #ascender := TRUE;
   #contador := 0;
   #X := 0;
   #Y := 0;
   #n := 0;
END_IF;

IF #Cooling = TRUE THEN
IF #n = 0 THEN
   IF #tempMatrix_0[#X, #Y] >= #maxTemp THEN

       "c2_TON_2".TON(IN := TRUE,
                   PT := t#10s);

       #Valvula := TRUE;

       IF "c2_TON_2".Q = TRUE THEN

           #Valvula := FALSE;

           //SIGUIENTE COORDENADA
           IF #ascender = TRUE THEN
               IF #Y = #Columnas_0 THEN
                   #X := #X + 1;
                   #ascender := FALSE;
               ELSE
                   #Y := #Y + 1;
               END_IF;
           ELSE
               IF #Y = 0 THEN
                   #X := #X + 1;
                   #ascender := TRUE;
               ELSE
                   #Y := #Y - 1;
               END_IF;
           END_IF;

           #contador := #contador + 1;
           IF #contador = ((#Filas_0 + 1) * (#Columnas_0 + 1)) THEN
               #contador := 0;
               #n := #n + 1;
               #X := 0;
               #Y := 0;
           END_IF;

       "c2_TON_2".TON(IN := FALSE,
                         PT := t#2s);
       END_IF;
   ELSE
       //SIGUIENTE COORDENADA
       IF #ascender = TRUE THEN
           IF #Y = #Columnas_0 THEN
               #X := #X + 1;
               #ascender := FALSE;
           ELSE
               #Y := #Y + 1;
           END_IF;
       ELSE
           IF #Y = 0 THEN
               #X := #X + 1;
               #ascender := TRUE;
           ELSE
               #Y := #Y - 1;
           END_IF;
       END_IF;

       #contador := #contador + 1;
       IF #contador = (#Filas_0 + 1) * (#Columnas_0 + 1) THEN
           #contador := 0;
           #n := #n + 1;
           #X := 0;
           #Y := 0;
       END_IF;

   END_IF;
END_IF;
IF #n = 1 THEN
   IF #tempMatrix_1[#X, #Y] >= #maxTemp THEN

       "c2_TON_2".TON(IN := TRUE,
                   PT := t#10s);

       #Valvula := TRUE;

       IF "c2_TON_2".Q = TRUE THEN

           #Valvula := FALSE;

           //SIGUIENTE COORDENADA
           IF #ascender = TRUE THEN
               IF #Y = #Columnas_1 THEN
                   #X := #X + 1;
                   #ascender := FALSE;
               ELSE
                   #Y := #Y + 1;
               END_IF;
           ELSE
               IF #Y = 0 THEN
                   #X := #X + 1;
                   #ascender := TRUE;
               ELSE
                   #Y := #Y - 1;
               END_IF;
           END_IF;

           #contador := #contador + 1;
           IF #contador = ((#Filas_1 + 1) * (#Columnas_1 + 1)) THEN
               #contador := 0;
               #n := #n + 1;
               #X := 0;
               #Y := 0;
           END_IF;

       "c2_TON_2".TON(IN := FALSE,
                         PT := t#2s);
       END_IF;
   ELSE
       //SIGUIENTE COORDENADA
       IF #ascender = TRUE THEN
           IF #Y = #Columnas_1 THEN
               #X := #X + 1;
               #ascender := FALSE;
           ELSE
               #Y := #Y + 1;
           END_IF;
       ELSE
           IF #Y = 0 THEN
               #X := #X + 1;
               #ascender := TRUE;
           ELSE
               #Y := #Y - 1;
           END_IF;
       END_IF;

       #contador := #contador + 1;
       IF #contador = (#Filas_1 + 1) * (#Columnas_1 + 1) THEN
           #contador := 0;
           #n := #n + 1;
           #X := 0;
           #Y := 0;
       END_IF;

   END_IF;
END_IF;
END_IF;

       IF (#n = #matrices) THEN
           #Cooling := FALSE;
           #n := 0;
       END_IF;

ELSE //Modo manual

   #Cooling := FALSE;
   #Activar := FALSE;

END_IF;

END_FUNCTION_BLOCK

